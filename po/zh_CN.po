#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:70
msgid "Source language"
msgstr "源语言"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:71
msgid "Language of the text to be translated"
msgstr "要被翻译的文本"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:75
msgid "Target language"
msgstr "目标语言"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:76
msgid "The language into which the text should be translated"
msgstr "要翻译成的语言"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:80
msgid "Split sentences"
msgstr "断句"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:81
msgid ""
"Sets whether the translation engine should first split the input into "
"sentences"
msgstr "设置翻译引擎是否首先把输入拆分成句子"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:85
msgid "Preserve formatting"
msgstr "保留格式"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:86
msgid ""
"Sets whether the translation engine should respect the original formatting, "
"even if it would usually correct some aspects"
msgstr "设置翻译引擎是否保持原始格式，即使它通常会纠正某些方面"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:90
msgid "Formality"
msgstr "形式检验"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:91
msgid ""
"Sets whether the translated text should lean towards formal or informal "
"language"
msgstr "设置翻译文本是应该倾向于正式文本，还是非正式文本"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:95
msgid "Url"
msgstr "网址"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:96
msgid "The url of DeepL"
msgstr "DeepL的网址"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:100
msgid "DeepL API key"
msgstr "DeepL的API key"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:101
msgid "Set DeepL API key"
msgstr "设置DeepL的API key"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:105
msgid "Shortcut"
msgstr "快捷键"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:106
msgid "Shortcut to translate the content of the clipboard"
msgstr "用于翻译剪切板中内容的快捷键"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:110
msgid "Show notifications"
msgstr "显示通知"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:111
msgid "Show notifications on translation"
msgstr "显示翻译通知"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:115
msgid "Dark theme"
msgstr "暗色模式"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:116
msgid "Enable dark theme otherwise light theme"
msgstr "启用暗色格式，否则采用亮色模式"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:120
msgid "Auto paste"
msgstr "自动粘贴"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:121
msgid "Paste automatically the content of the clipboard in the input text box"
msgstr "自动将剪切板中的内容粘贴到输入框中"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:125
msgid "Auto copy"
msgstr "自动复制"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:126
msgid "Copy automatically the content of the output box in the clipboard"
msgstr "自动将输出框中的内容复制到剪切板中"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:130
msgid "Auto translate"
msgstr "自动翻译"

#: schemas/org.gnome.shell.extensions.translate-assistant.gschema.xml:131
msgid ""
"Translate automatically the content of the input box and the set the result "
"in the output box"
msgstr "自动翻译输入框中的内容，并将结果放入输出框"

#: dialogwidgets.js:53
msgid "Cancel"
msgstr "取消"

#: dialogwidgets.js:54
msgid "Ok"
msgstr "确定"
